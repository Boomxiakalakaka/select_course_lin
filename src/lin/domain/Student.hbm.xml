<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="lin.domain">
	<class  name="Student" table="student">
	<cache usage="read-write"/>
	<!-- 主键策略 -->
	<id name="student_id" type="java.lang.Integer">
	<generator class="identity"/>  
	</id>
	<property name="student_name" type="java.lang.String">
	<column name="student_name" length="32" />
	</property>
	<property name="student_gender" type="java.lang.Integer">
	<column name="student_gender" length="32" />
	</property>
	<property name="idcard" type="java.lang.String">
	<column name="idcard" length="32"/>
	</property>
	<property name="student_password" type="java.lang.String">
	<column name="student_password" length="32" />
	</property>
	<property name="department_id" type="java.lang.Integer">
	<column name="department_id" length="32" />
	</property>
	<property name="specialty_id" type="java.lang.Integer">
	<column name="specialty_id" length="32" />
	</property>
	<property name="politics" type="java.lang.String">
	<column name="politics" length="6" />
	</property>
	<property name="photoaddress" type="java.lang.String">
	<column name="photoaddress" length="32" />
	</property>
	<property name="phonenumber" type="java.lang.Integer">
	<column name="phonenumber" length="32" />
	</property>
	<property name="mailbox" type="java.lang.String">
	<column name="mailbox" length="32" />
	</property>
	<property name="qqnumber" type="java.lang.Integer">
	<column name="qqnumber" length="20" />
	</property>
	<property name="familyaddress" type="java.lang.String">
	<column name="familyaddress" length="32" />
	</property>
	</class>
	
</hibernate-mapping>
