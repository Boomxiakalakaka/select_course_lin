<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="lin.domain">
	<class  name="Teacher" table="teacher">
	<cache usage="read-write"/>
	<!-- 主键策略 -->
	<id name="teacher_id" type="java.lang.Integer">
	<generator class="identity"/>  
	</id>
	<property name="teacher_name" type="java.lang.String">
	<column name="teacher_name" length="32" />
	</property>
	<property name="teacher_gender" type="java.lang.Integer">
	<column name="teacher_gender" />
	</property>
	<property name="teacher_idcard" type="java.lang.String">
	<column name="teacher_idcard" length="32"/>
	</property>
	<property name="teacher_post" type="java.lang.String">
	<column name="teacher_post" length="32" />
	</property>
	<property name="entry_time" type="timestamp">
	<column name="entry_time" length="32" />
	</property>
	<property name="teacher_password" type="java.lang.String">
	<column name="teacher_password" length="32" />
	</property>
	<property name="phone_number" type="java.lang.String">
	<column name="phone_number" length="32" />
	</property>
	<property name="photoaddress" type="java.lang.String">
	<column name="photoaddress" length="32" />
	</property>
	<property name="familyaddress" type="java.lang.String">
	<column name="familyaddress" length="32" />
	</property>
	<property name="introduce" type="java.lang.String">
	<column name="introduce" length="200" />
	</property>
	</class>
</hibernate-mapping>
